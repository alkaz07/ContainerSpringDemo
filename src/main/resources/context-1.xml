<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="watchman" class="parks.Watchman">
        <constructor-arg value="Михалыч"/>
    </bean>
    <bean id="park" class="parks.Park">
        <constructor-arg name="id" value="3"/>
        <constructor-arg name="name" value="Баболовский"/>
    </bean>
    <bean id="park2" class = "parks.Park">
        <constructor-arg name="id" value="33"/>
        <constructor-arg name="name" value="Федоровский"/>
    </bean>
</beans>